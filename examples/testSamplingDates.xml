    <BirthDeathSkylineModel spec="BirthDeathSkylineModel" id="birthDeath" tree="@tree" intervalNumber="2">
        <parameter name="orig_root" id="orig_root" value ="0.1" lower="0." upper="100000."/>
        <parameter name="R0" id="R0" value="2 1.5" lower="0."/>
        <parameter name="becomeUninfectiousRate" id="becomeUninfectiousRate" value="3. 2." lower="0."/>
        <parameter name="samplingProportion" id="samplingProportion" value=".001 .0012" lower="0." upper="1."/>
        <intervalTimes spec="RealParameter" id="intervalTimes" value="0. 1." />
    </BirthDeathSkylineModel>

    <tree spec="beast.util.ClusterTree" id="tree" clusterType="upgma" trait="@tipDates">
        <taxa idref="alignment"/>
    </tree>

    <distribution id="prior" spec="util.CompoundDistribution">
        <distribution id="BDlikelihood" spec="util.CompoundDistribution">
            <distribution idref="birthDeath"/>
        </distribution>
        <distribution id="tipDatePrior" monophyletic="false" spec="beast.math.distributions.MRCAPrior" tipsonly="true" tree="@tree" taxonset="@tips">
            <distr id="Uniform1" lower="0.0" offset="0.0" spec="beast.math.distributions.Uniform" upper="Infinity"/>
        </distribution>
    </distribution>


    <run chainLength="300000000" id="mcmc" spec="MCMC" storeEvery="1000">

        <state id="state" storeEvery="100000">
            <stateNode idref='tree'/>
            <stateNode idref="orig_root"/>
            <stateNode idref="intervalTimes"/>
        </state>

        <distribution idref="prior"/>

        <operator id='origOperator' spec='ScaleOperator' scaleFactor=".75" weight="1" parameter="@orig_root"/>
        <!--operator id='timeOperator' spec='ScaleOperator' scaleFactor=".75" weight="1" parameter="@intervalTimes"/-->

        <operator id='treeScaler' spec='ScaleOperator' scaleFactor=".75" weight="2" tree="@tree" rootOnly="true"/>
        <operator id='treeScaler2' spec='ScaleOperator' scaleFactor=".75" weight="20" tree="@tree"/>
        <operator spec='Uniform' weight="20" tree="@tree"/>
        <operator spec='SubtreeSlide' weight="1" gaussian="true" size="1.0" tree="@tree"/>
        <operator id='narrow' spec='Exchange' isNarrow='true' weight="1" tree="@tree"/>
        <operator id='wide' spec='Exchange' isNarrow='false' weight="1" tree="@tree"/>
        <operator spec='WilsonBalding' weight="1" tree="@tree"/>

        <operator id="TipDatesRandomWalker" windowSize="1" spec="TipDatesRandomWalker" taxonset="@tips" tree="@tree" weight="1"/>

        <logger fileName="BDSKY_treeSampler_Revision_B2_$(seed).log" id="log" logEvery="10000" mode="autodetect" model="@prior">
            <distribution idref="prior" name="log"/>
            <log idref="BDlikelihood"/>
            <log spec='beast.evolution.tree.TreeHeightLogger' tree='@tree'/>
            <log idref="orig_root"/>
            <log idref="intervalTimes"/>
        </logger>

        <logger id="screenlog" logEvery="10000" mode="autodetect">
            <distribution idref="prior" name="log"/>
            <log arg="@prior" id="ESS.0" spec="util.ESS"/>
            <log spec='beast.evolution.tree.TreeHeightLogger' tree='@tree'/>
            <log idref="BDlikelihood"/>
            <log idref="orig_root"/>
            <log idref='TipDatesLogger'/>
            <log idref="intervalTimes"/>
        </logger>

        <logger fileName="BDSKY_treeSampler_Revision_B2_tipDates_$(seed).log" id="tiplog" logEvery="1000" mode="autodetect" model="@prior">
            <log spec='beast.math.distributions.MRCAPrior' tipsonly='true' id='TipDatesLogger' taxonset="@tips" tree='@tree'/>
        </logger>

        <logger fileName="BDSKY_treeSampler_Revision_B2_$(seed).trees" id="treelog" logEvery="10000" mode="tree">
            <tree idref="tree" name="log"/>
        </logger>

    </run>

</beast>
